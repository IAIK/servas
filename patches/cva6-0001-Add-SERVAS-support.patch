+            store_urange_offset           = data_vaddr - enclave_config_i.urange.vbase;
+            store_effective_tweak.voffset[riscv::SV-1-DCACHELINE_WIDTH_BIT:0] = store_urange_offset[riscv::SV-1 :  DCACHELINE_WIDTH_BIT];
+        end else begin
