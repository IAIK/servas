+            if (ls_state_tweak.range_map.mrange == 1'b1) begin
+                ls_state_tweak.SID.S1 = enclave_config_i.sid_shcode[riscv::ENCLAVE_TWEAK_SID1_BITSIZE-1:0];
+            end else if (ls_state_tweak.range_map.srange == 1'b1) begin
