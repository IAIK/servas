+        load_effective_tweak.pte_msb.SID_selector  = dtlb_pte_entry_i.msb.enclave_sid_selection;
+        store_effective_tweak.pte_msb.SID_selector = dtlb_pte_entry_i.msb.enclave_sid_selection;
