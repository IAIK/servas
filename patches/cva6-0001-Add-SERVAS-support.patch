+  signal StateAfterRho   : LaneArray;
+  signal StateAfterPi    : LaneArray;
+  signal StateAfterChi   : LaneArray;
+  signal StateAfterIota  : LaneArray;
+begin
+  theta : entity work.keccak_round_parallel_theta
+    port map (
+      StatexDI => StatexDI,
