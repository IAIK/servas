+module enclave_effective_tweak import ariane_pkg::*; #(
+    parameter ariane_pkg::ariane_cfg_t ArianeCfg = ariane_pkg::ArianeDefaultConfig
+) (
+    input  logic                     clk_i,    // Clock TBD
